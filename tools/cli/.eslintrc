{
	"extends": [
		"marine/prettier/node",
		"eslint:recommended"
	],
	
	"parserOptions": {
		"project": "./tsconfig.json",
		"ecmaVersion": 2021
	},
	"ignorePatterns": ["**/lib/*"],
	"env": {
		"node": true,
		"es6": true
	},
	"rules": {
		"no-redeclare": 0,
		"@typescript-eslint/naming-convention": 0
	}
}