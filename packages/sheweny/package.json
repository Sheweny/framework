{
  "name": "sheweny",
  "version": "4.2.1",
  "description": "The powerful framework for create discord bots",
  "main": "./build/index.js",
  "type": "commonjs",
  "types": "./typings/index.d.ts",
  "scripts": {
    "build": "tsc && tsc --build tsconfig.typings.json",
    "build:typings": "tsc --build tsconfig.typings.json",
    "format": "prettier --write . && eslint src --fix",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "dev": "tsnd --respawn --transpile-only --cls ./test/ts/src/index.ts",
    "publish:dev": "npm publish --tag dev"
  },
  "author": "Smaug#6739",
  "license": "MIT",
  "keywords": [
    "discord",
    "discord.js",
    "framework",
    "discord-bots",
    "bots",
    "cli",
    "discordapp",
    "api",
    "bot",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Sheweny/framework.git"
  },
  "homepage": "https://sheweny.js.org",
  "dependencies": {
    "@sheweny/resolve": "^1.3.2",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "discord.js": "^14.9.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}
